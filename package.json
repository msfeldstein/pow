    "dotenv": "^16.3.1",
    "epubjs": "^0.3.93",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "node-unrar-js": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-reader": "^1.2.1",
    "react-router-dom": "^6.15.0",
    "react-spinners": "^0.13.8",
    "sharp": "^0.32.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite-express": "*",
    "@vitejs/plugin-react": "^3.0.1"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.0",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
